{
  "alias": [
    "www.reussitess.fr",
    "reussitess.fr"
  ],
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "reussitess.fr"
        }
      ],
      "destination": "https://www.reussitess.fr/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
